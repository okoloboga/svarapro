#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/code/svara-pro/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@_fa0d3c220ec27571612367cd35470923/node_modules/typeorm/node_modules:/home/user/code/svara-pro/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@_fa0d3c220ec27571612367cd35470923/node_modules:/home/user/code/svara-pro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/code/svara-pro/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@_fa0d3c220ec27571612367cd35470923/node_modules/typeorm/node_modules:/home/user/code/svara-pro/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@_fa0d3c220ec27571612367cd35470923/node_modules:/home/user/code/svara-pro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-commonjs.js" "$@"
fi
